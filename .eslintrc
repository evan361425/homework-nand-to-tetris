{
  "env": {
    "es6": true
  },
  "extends": [
    "google"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "max-len": ["warn", {"code": 200, "tabWidth": 2}],
    "object-curly-spacing": ["error", "always"],
    "require-jsdoc": "off",
    "linebreak-style": 0
  }
}
